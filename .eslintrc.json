{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "plugins": [
        "no-use-extend-native",
        "import",
        "security"
      ],
      "rules": {
        "no-use-extend-native/no-use-extend-native": "error",
        "security/detect-non-literal-regexp": "warn"
      }
}
